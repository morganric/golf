<%= simple_form_for(@club) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :about %>
    <%= f.label :banner %>
    <%= f.file_field :banner %>
    <%= f.label :logo %>
    <%= f.file_field :logo %>
    <%= f.label :address %>
    <br/>
    <%= f.text_area :address %>
    <%= f.input :zip_code %>
    <%= f.input :country %>
    <%= f.input :phone_number %>
    <%= f.input :green_fees %>
    <%= f.input :email %>
    <%= f.input :live %>
    <%= f.input :latitude, as: :hidden %>
    <%= f.input :longitude, as: :hidden %>
  </div>
  <hr/>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
